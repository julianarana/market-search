{
    "compilerOptions": {
      "target": "es5",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": false,
      "jsx": "react",
      "baseUrl": "."
    },
    "include": [
      "src",
      "**/*.ts",
      "**/*.tsx"
, "config/webpack/webpack.prod.config.js", "config/webpack/webpack.dev.config.js", "webpack.config.js", "webpack.config.js"    ],
    "exclude": [
      "node_modules"
    ]
  }
  